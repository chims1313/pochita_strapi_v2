{
  "kind": "collectionType",
  "collectionName": "citas",
  "info": {
    "singularName": "cita",
    "pluralName": "citas",
    "displayName": "Cita"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "fecha": {
      "type": "datetime"
    },
    "motivo": {
      "type": "text"
    },
    "estado": {
      "type": "enumeration",
      "enum": [
        "pendiente",
        "confirmada",
        "completada",
        "cancelada"
      ]
    },
    "notas_veterinario": {
      "type": "text"
    },
    "cliente": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "citas_como_cliente"
    },
    "veterinario": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "cita"
    },
    "users_permissions_user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "cliente"
    },
    "mascota": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::mascota.mascota",
      "inversedBy": "citas"
    }
  }
}
